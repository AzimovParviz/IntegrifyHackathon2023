[32m+[m[32m// Fetch all users from API[m
[32m+[m[32mexport const fetchAllUsers = createAsyncThunk("fetchAllUsers", async () => {[m
[32m+[m[32m  try {[m
[32m+[m[32m    const res: AxiosResponse<User[] | Error, any> = await axiosInstance.get("users");[m
[32m+[m[32m    if (!(res.data instanceof Error)) return res.data;[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
