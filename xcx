[32m+[m[32mimport { configureStore, ThunkAction, Action } from "@reduxjs/toolkit";[m
[32m+[m[32mimport { TypedUseSelectorHook, useDispatch, useSelector } from 'react-redux';[m
[32m+[m[32mimport type { RootState, AppDispatch } from '../redux/store';[m
[32m+[m[32mimport { projectReducer } from "./reducers/projectReducer";[m
[32m+[m[32mimport { userReducer } from "./reducers/userReducer";[m
[32m+[m[32mimport { categoryReducer } from "./reducers/categoryReducer";[m
[32m+[m[32mimport { taskReducer } from "./reducers/taskReducer";[m
[32m+[m
[32m+[m
[32m+[m[32mexport const store = configureStore({[m
[32m+[m[32m  reducer: {[m
[32m+[m[32m    projectReducer,[m
[32m+[m[32m    userReducer,[m
[32m+[m[32m    categoryReducer,[m
[32m+[m[32m    taskReducer[m
[32m+[m[32m  },[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mexport type AppDispatch = typeof store.dispatch;[m
[32m+[m[32mexport type RootState = ReturnType<typeof store.getState>;[m
[32m+[m[32mexport type AppThunk<ReturnType = void> = ThunkAction<ReturnType, RootState, unknown, Action<string>>;[m
[32m+[m
[32m+[m[32m// Use throughout your app instead of plain `useDispatch` and `useSelector`[m
[32m+[m[32mexport const useAppDispatch = () => useDispatch<AppDispatch>();[m
[32m+[m[32mexport const useAppSelector: TypedUseSelectorHook<RootState> = useSelector;[m
